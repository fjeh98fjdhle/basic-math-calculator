true_map = {(0, 1): False, (2, 7): True, (7, 12): True, (12, 19): True, (19, 22): False}
true_map_2 = {}
true_map_3 = {}
false_map = {}


count = 0
for a in true_map:
    count += 1 
    value = true_map[a]
    value = str(value) + str(count)
    false_map[count] = a
#   1: (0, 1)  false_map


count = 0
for a in true_map: 
    count += 1
    value = true_map[a]
    true_map_2[count] = value
#   1: False  


list_map = []


#for a in range(5): 
#    true_map_3[true_map_2[a]] = false_map[a]

for a in true_map_2: 
    list_map.append(a)

print(true_map_2) # we map 1 to 5 to the correspoding boolean values.
print(false_map) # we map 1 to 5 to the corresponding + indexes.
print(true_map) # we map + indexes to the corresponding boolean values
together_value = []
stationary_list = []
list_for_map = []

count = 0 
for n in list_map: 
    count += 1
    if count < len(list_map) -1: 
        if true_map_2[count] == true_map_2[count +1]: 
            print("True and True")
            tuple = false_map[count]
            for a in tuple:
                list_for_map.append(a)
            for a in list_for_map[:1]:
                print(a)
                stationary_list.append(a)
            list_for_map.clear()

            tuple = false_map[count +1]
            for a in tuple:
                list_for_map.append(a)
            for a in list_for_map[1:]:
                stationary_list.append(a)
            list_for_map.clear
            for n in stationary_list:
                together_value.append(a)
            print("together_value")
            print("this is stationary_list")
            print(stationary_list)
            stationary_list.clear()
            
        else: 
            for n in false_map[count]:
                together_value.append(n)
                print(together_value)
                print()
    elif count == len(list_map) -1:
        a = false_map[count]                
        for a in a:
            together_value.append(a)

